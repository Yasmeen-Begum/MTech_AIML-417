#include<stdio.h>
#include<time.h>
int binarySearch(int arr[], int l, int r, int x){
if (r >= l){
int mid = l + (r - l)/2;
if (arr[mid] == x) return mid;
if (arr[mid] > x) return binarySearch(arr, l, mid-1, x);
return binarySearch(arr, mid+1, r, x);
return -1;}}
int main(void){
clock_t start,end;
double execution_time;
start=clock();
int arr[11500];
FILE *fptr;
int i=0,index,k;
fptr=fopen("inputFile.txt","r");
while((fscanf(fptr,"%d",&arr[i]))!=EOF){
i++;}
fclose(fptr);
printf("Enter element to search:");
scanf("%d",&k);
int n= sizeof(arr)/sizeof(arr[0]);
index = binarySearch(arr, 0, n-1, k);
if(index != -1)
printf("Element %d is present at index %d",k, index);
else
printf("Element %d is not present",k);
end=clock();
execution_time=((double)(end-start))/CLOCKS_PER_SEC;
printf("\nTime taken to execute in second: %f",execution_time);
return 0;}
