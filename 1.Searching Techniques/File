1.1 Create a file more than 10,000 elements generate using random function

#include<stdio.h>
#include<stdlib.h>
int main()
{
int size = 11500, last =0;
FILE *fptr;
fptr = fopen("inputFile.txt", "w");
if (fptr == NULL){
printf("ERROR Creating File!");
exit(1);
}
while(size-- >0) {
last += (rand()%1000);
fprintf(fptr,"%d \t",last);
}
printf("file created successfully");
fclose(fptr);
return 0;
}
